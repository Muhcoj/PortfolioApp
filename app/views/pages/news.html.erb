<main role="main" class="inner cover">
  <div class="row">
    <div class="col-md-12">
      <h1 class="cover-heading">News.</h1>
    </div>
    <% @tweets.each do |tweet| %>
      <div class="col-md-6">
        <div class="news">
          <div class="card">
            <%= twitter_parser tweet %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</main>

